<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping
xmlns="urn:nhibernate-mapping-2.2"
assembly="KupiProdam"
namespace="KupiProdam.Entities.Entites"
auto-import="true">

    <class name="KupiProdam.Entities.Entites.Seller, KupiProdam.Entities" table="seller">
    
    <id name="Id" 
        access="property" 
        column="Id" 
        type="Int32">
      <generator class="native"></generator>
    </id>
    
    <property name="Name" 
              access="property"
              column="Name" 
              type="String">
    </property>
    
    <property name="Password" 
              access="property"
              column="Password" 
              type="String">
    </property>
    
    <property name="MainPhone" 
              access="property"
              column="MainPhone" 
              type="String">
    </property>
    
    <property name="Email" 
              access="property"
              column="Email" 
              type="String">
    </property>

    <property name="About"
              access="property"
              column="About"
              type="String">
    </property>

    <property name="Photo"
              access="property"
              column="Photo"
              type="String">
    </property>
    
    <property name="Skype"
              access="property"
              column="Skype"
              type="String">
    </property>
    
    <property name="VKontakte"
              access="property"
              column="VKontakte"
              type="String">
    </property>
    
    <property name="Facebook"
              access="property"
              column="Facebook"
              type="String">
    </property>
    
  </class>
</hibernate-mapping>